{{!-- shows list of submitted tags --}}
<ul class="taglist">
    <li class="clickme">
        <a href="#new">Add New Bookmark</a>
    </li>
    <li class="clickme">
        <a href="#">Show All Bookmarks</a>
    </li>
    {{#each this as |tag|}}
     <li>
         <a href="#{{tag}}">{{ tag }}</a>
     </li>
    {{/each}}
</ul>
